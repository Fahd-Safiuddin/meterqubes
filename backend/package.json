  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-watch": "^7.0.0",
    "eslint": "^5.14.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "supertest": "^3.4.2"
  },
  "scripts": {
    "watch": "babel-watch --watch src src/index.js",
    "start": "babel-node src/index.js",
    "debug": "nodemon --exec babel-node -e js,json,yml --inspect src/index.js",
    "test": "jest --ci --silent --passWithNoTests --forceExit",
    "test-dev": "jest --detectOpenHandles",
    "start:dev": "NODE_ENV=development run-s db:migrate watch",
    "start:prod": "NODE_ENV=production run-s db:migrate start",
    "start:stage": "NODE_ENV=staging run-s db:migrate start",
    "start:debug": "NODE_ENV=development run-s db:migrate debug",
    "start:test": "NODE_ENV=test run-s db:init test db:drop",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "db:clean": "node ./src/db/index.js init",
    "db:drop": "node ./src/db/index.js drop",
    "db:migrate": "sequelize db:migrate && sequelize db:seed:all",
    "db:init": "run-s db:clean db:migrate"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 10"
  },
